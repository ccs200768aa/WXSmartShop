<view class='container'>
  <view class='column-group'>
    <view class='column'>
      <text>{{my_avater}}</text>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    </view>
  </view>
  <view class='column-group'>
    <view class='column'>
      <text>{{my_nickName}}</text>
      <text>{{userInfo.nickName}}</text>
    </view>
    <view class='column'>
      <text>{{my_sex}}</text>
      <text wx:if='{{userInfo.gender}}==1'>男</text>
      <text wx:else>女</text>
    </view>
    <view class='column'>
      <text>{{my_shippingAddress}}</text>
      <text class='icon-next'></text>
    </view>
  </view>
  <view class='column-group'>
    <view class='column'>
      <text class=''>{{my_contact}}</text>
      <text>{{gt}}</text>
    </view>
  </view>
  <view class='column-group'>
    <view class='column'>
      <text>{{my_help}}</text>
      <text>{{gt}}</text>
    </view>
  </view>
</view>